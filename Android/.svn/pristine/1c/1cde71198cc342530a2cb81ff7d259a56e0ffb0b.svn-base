package com.android.pgb.Bean;

import com.android.pgb.Utils.JSONUtils;

import org.json.JSONObject;

/**
 * Created by kanghui on 2017/3/16.
 */

public class PGJG {
    public PGJG(JSONObject obj) {
        this.pgzj = JSONUtils.getInt(obj,"jzbz",100);
        this.pgmc =  JSONUtils.getString(obj,"jgmc");;
    }

    private  int pgzj;
    private  String pgmc;
    public int getPgzj() {
        return pgzj;
    }

    public void setPgzj(int pgzj) {
        this.pgzj = pgzj;
    }

    public String getPgmc() {
        return pgmc;
    }

    public void setPgmc(String pgmc) {
        this.pgmc = pgmc;
    }



}
