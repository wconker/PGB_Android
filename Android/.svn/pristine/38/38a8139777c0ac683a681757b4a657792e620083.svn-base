package com.android.pgb.Bean;

import com.android.pgb.Utils.JSONUtils;

import org.json.JSONObject;

/**
 * Created by kanghui on 2017/3/23.
 */

public class TaskBean {
    /**
     * tbid : 59
     * zbid : 94
     * tbuserid : 2
     * tbsj : 2017-03-21 13:53
     * bdw : zzzzzzzz
     * zbj : 0.00
     * tbsm : 内部指定
     * fjbs :
     * tbj : 0.00
     * zbbz : 1
     * xxsm : 1111111
     * bc : 0.00
     * ztms : 已定标
     */

    private int tbid;
    private int zbid;
    private int tbuserid;
    private String tbsj;
    private String bdw;
    private String zbj;
    private String tbsm;
    private String fjbs;
    private String tbj;
    private int zbbz;
    private String xxsm;
    private String bc;
    private String ztms;

    public TaskBean(JSONObject obj) {

        this.setTbid(JSONUtils.getInt(obj, "tbid", 0));
        this.setZbid(JSONUtils.getInt(obj, "zbid", 0));
        this.setTbuserid(JSONUtils.getInt(obj, "tbuserid", 0));
        this.setZbbz(JSONUtils.getInt(obj, "zbbz", 0));
        this.setTbsj(JSONUtils.getString(obj, "tbsj"));
        this.setBdw(JSONUtils.getString(obj, "bdw"));
        this.setZbj(JSONUtils.getString(obj, "zbj"));
        this.setTbsm(JSONUtils.getString(obj, "tbsm"));
        this.setFjbs(JSONUtils.getString(obj, "fjbs"));
        this.setTbj(JSONUtils.getString(obj, "tbj"));
        this.setXxsm(JSONUtils.getString(obj, "xxsm"));
        this.setBc(JSONUtils.getString(obj, "bc"));
        this.setZtms(JSONUtils.getString(obj, "ztms"));


    }


    public int getTbid() {
        return tbid;
    }

    public void setTbid(int tbid) {
        this.tbid = tbid;
    }

    public int getZbid() {
        return zbid;
    }

    public void setZbid(int zbid) {
        this.zbid = zbid;
    }

    public int getTbuserid() {
        return tbuserid;
    }

    public void setTbuserid(int tbuserid) {
        this.tbuserid = tbuserid;
    }

    public String getTbsj() {
        return tbsj;
    }

    public void setTbsj(String tbsj) {
        this.tbsj = tbsj;
    }

    public String getBdw() {
        return bdw;
    }

    public void setBdw(String bdw) {
        this.bdw = bdw;
    }

    public String getZbj() {
        return zbj;
    }

    public void setZbj(String zbj) {
        this.zbj = zbj;
    }

    public String getTbsm() {
        return tbsm;
    }

    public void setTbsm(String tbsm) {
        this.tbsm = tbsm;
    }

    public String getFjbs() {
        return fjbs;
    }

    public void setFjbs(String fjbs) {
        this.fjbs = fjbs;
    }

    public String getTbj() {
        return tbj;
    }

    public void setTbj(String tbj) {
        this.tbj = tbj;
    }

    public int getZbbz() {
        return zbbz;
    }

    public void setZbbz(int zbbz) {
        this.zbbz = zbbz;
    }

    public String getXxsm() {
        return xxsm;
    }

    public void setXxsm(String xxsm) {
        this.xxsm = xxsm;
    }

    public String getBc() {
        return bc;
    }

    public void setBc(String bc) {
        this.bc = bc;
    }

    public String getZtms() {
        return ztms;
    }

    public void setZtms(String ztms) {
        this.ztms = ztms;
    }
}
