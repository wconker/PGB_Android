<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="8dp"
    android:background="#f2f3f4"
    android:descendantFocusability="blocksDescendants"
    android:orientation="vertical"
>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="1dp"
        android:background="@color/white"
        android:gravity="center"
        android:orientation="vertical"
        android:padding="15dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="8dp"
            android:background="@color/white"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/bdw"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="杭州市滨江区太阳国际公寓2-1-1902室 85方"
                android:textSize="@dimen/text_size_level3"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="24dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/zblbmc"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/button_border"
                android:gravity="center"
                android:includeFontPadding="false"
                android:padding="5dp"
                android:text="住宅"
                android:textColor="@color/black"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/zbfw"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/button_border"
                android:gravity="center"
                android:includeFontPadding="false"
                android:padding="5dp"
                android:text="对外"
                android:textColor="@color/black"
                android:textSize="10dp" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:layout_gravity="center"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"

            android:orientation="horizontal">

            <TextView
                android:id="@+id/ztms"
                android:layout_width="55dp"
                android:layout_height="20dp"
                android:layout_gravity="bottom"
                android:background="@drawable/button_press"
                android:gravity="center"
                android:text="已定标"
                android:textColor="@color/white"
                android:textSize="13dp"
                android:textStyle="bold" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_gravity="bottom"
                android:layout_marginLeft="5dp"
                android:src="@drawable/redbag" />

            <TextView
                android:id="@+id/info_bc"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="8dp"
                android:gravity="bottom"
                android:text="￥15"
                android:textColor="#e64340" />


            <!--<RelativeLayout-->

            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="wrap_content">-->

            <!--<TextView-->
            <!--android:id="@+id/downTime"-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_alignParentEnd="true"-->
            <!--android:layout_alignParentRight="true"-->
            <!--android:textSize="@dimen/text_size_level5"-->
            <!--android:text="933秒" />-->
            <!--</RelativeLayout>-->
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="8dp"
            android:orientation="horizontal">


            <TextView
                android:id="@+id/zbsj"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="2017-03-08 16:19"
                android:textSize="12dp" />


            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:gravity="center"
                android:text="至"
                android:textColor="#dc9d20"
                android:textSize="12dp" />

            <TextView
                android:id="@+id/zbyqsj"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="2017-03-08 16:19"
                android:textSize="12dp" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:gravity="center"
                android:text="阅读次数："
                android:textSize="12dp" />

            <TextView
                android:id="@+id/info_readCount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="6"
                android:textSize="12dp" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="8dp"
            android:orientation="vertical">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="投标信息" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:background="@color/gray" />


            <com.android.pgb.View.InnerListview.view.ChildListView
                android:id="@+id/info_listview"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:background="@color/white"
                android:padding="2dp">

            </com.android.pgb.View.InnerListview.view.ChildListView>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="14dp"
                android:layout_marginTop="5dp"
                android:orientation="horizontal">


                <Button
                    android:id="@+id/stop"
                    android:layout_width="61dp"
                    android:layout_height="23dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_margin="5dp"
                    android:layout_marginRight="5dp"
                    android:background="@drawable/button_press"
                    android:text="终止招标"
                    android:textColor="@color/white"
                    android:textSize="12dp" />


            </RelativeLayout>
        </LinearLayout>


    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginLeft="15dp"
        android:background="@color/gray" />

</LinearLayout>