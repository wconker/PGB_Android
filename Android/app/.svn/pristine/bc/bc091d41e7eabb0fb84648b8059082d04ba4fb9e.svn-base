<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:gravity="center"
        android:orientation="vertical"
        android:padding="15dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="8dp"
            android:background="@color/white"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/bdw"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="杭州市滨江区太阳国际公寓2-1-1902室 85方"
                android:textSize="@dimen/text_size_level3"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="8dp"
            android:background="@color/white"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/bz"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textColor="@color/black_wkh"
                android:textSize="@dimen/text_size_level3"
                android:textStyle="italic" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="26dp"
            android:orientation="horizontal">

            <TextView

                android:id="@+id/zbj"
                android:paddingRight="3dp"
                android:paddingLeft="3dp"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/button_border"
                android:gravity="center"
                android:includeFontPadding="false"
                android:text="招标价:233元"
                android:textColor="@color/black_wkh"
                android:textSize="12dp"
                />

            <TextView
                android:padding="3dp"
                android:id="@+id/zt"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_marginLeft="10dp"
                android:background="@color/colorPrimary"
                android:gravity="center"
                android:includeFontPadding="false"
                android:text="已终止"
                android:textColor="@color/white"
                android:textSize="12dp"
              />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="26dp"
            android:layout_marginTop="5dp"
            android:layout_gravity="center"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:orientation="horizontal">


            <TextView

                android:id="@+id/tbj"
                android:paddingRight="3dp"
                android:paddingLeft="3dp"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:background="@drawable/button_border"
                android:gravity="center"
                android:includeFontPadding="false"
                android:text="投标价:233元"
                android:textColor="@color/black_wkh"
                android:textSize="12dp"
                />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="15dp"
                android:src="@drawable/success" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="8dp"
            android:orientation="horizontal">


            <!--<TextView-->
            <!--android:id="@+id/info_lastTime"-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:gravity="center"-->
            <!--android:text="2017-03-08 16:19"-->
            <!--android:textSize="12dp" />-->


            <!--<TextView-->
            <!--android:id="@+id/info_zt"-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_marginLeft="8dp"-->
            <!--android:gravity="center"-->
            <!--android:text="询价中"-->
            <!--android:textColor="#dc9d20"-->
            <!--android:textSize="12dp" />-->


            <TextView
                android:id="@+id/zbsj"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="2017-3-11 15:44"
                android:textSize="13dp" />


        </LinearLayout>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginLeft="15dp"
        android:background="@color/gray" />
</LinearLayout>